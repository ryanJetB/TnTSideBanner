<div class="containerSmallBanner">
    <div class="headerTitle">
        <p class="headerText">The JetBlue experience.</p>
    </div>
    <div id="icons">
        <div id="flyFiIcon" class="icons">
            <div id="overlayflyFiIcon" class="overlay">
                <p class="iconTitle">Free Fly-Fi<sup>®</sup>***</p>
            </div>
            <p id="flyFiLegal" class="overlayLegal">***Available on most aircraft, with more installations underway.</p>
        </div>
        <div id="smileyIcon" class="icons">
            <div id="overlaysmileyIcon" class="overlay">
                <p class="iconTitle">Free DIRECTV<sup>®</sup> & SiriusXM Radio at every seat**</p>
            </div>
            <p id="smileyLegal" class="overlayLegal">**DIRECTV and SiriusXM service is not available on flights outside the continental U.S.</p>
        </div>
        <div id="legRoomIcon" class="icons">
            <div id="overlaylegRoomIcon" class="overlay">
                <p class="iconTitle">Most legroom in coach*</p>
            </div>
            <p id="legRoomLegal" class="overlayLegal">*Based on average fleet-wide seat pitch of U.S. airlines</p>
        </div>
        <div id="snacksIcon" class="icons">
            <div id="overlaysnacksIcon" class="overlay">
                <p class="iconTitle">Free snacks & drinks</p>
            </div>
        </div>
        <div id="awardIcon" class="icons">
            <div id="overlayawardIcon" class="overlay">
                <p class="iconTitle">Award-winning service</p>
            </div>
        </div>
    </div>
</div>
		
<% Response.AddHeader "Access-Control-Allow-Origin", "*" %>